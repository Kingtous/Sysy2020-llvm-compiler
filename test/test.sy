int param = 100;
// 求[1,param)整数和
int getNum(int param){
//    param = 1;
    int cnt = 0;
    for(int index=0; index<param; index=index+1){
        if (index % 2 == 0){
            if (index % 3 == 0){
                cnt = cnt + index;
            }
        }
        cnt = cnt + index;
    }
    return cnt;
}
