int param = 100;
int nums[10][10][10][10];
// 求[1,param)整数和
int getNum(int param){
    int cnt = 0;
    for(int index=0; index<param; index=index+1){
        if (index % 2 == 0){
            if (index % 3 == 0){
                cnt = cnt + index;
            }
        }
        cnt = cnt + index;
        nums[6][2][0][1] = cnt;
        nums[6][2][0][1] = nums[6][2][0][2] + nums[6][1][0][2];
    }
    return cnt;
}
