int qsort(int a[],int left,int right){
    if (right <= left){
        ret -1;
    }
    int base = a[left];
    int j = left;
    int k = right;
    echo('begin wh');
    wh(j<k){
        wh(a[k]>=base && j<k){
            k = k - 1;
            echo('k=k-1');
        }
        a[j] = a[k];
        wh(a[j]<=base && j<k){
            j = j + 1;
            echo('j=j+1');
        }
        a[k] = a[j];
        echo('j,k,a[j],a[k]',j,k,a[j],a[k]);
    }
    a[k] = base;
    qsort(a,left,k);
    qsort(a,left,k- 1);
    ret 0;
}

int main(){
    int b[5]= {4,7,1,2,3};
    int max = 4;
    echo('start qsort');
    qsort(b,0,max);
    echo('output qsort result');
    lp(int i=0;i<=max;i=i+1){
        echo(b[i]);
    }
}